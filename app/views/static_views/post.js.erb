<% case  @type %>
<% when 'diagnostic' %>
	$('#pageslide').html('<%= escape_javascript(render(partial: "/diagnostics/form", locals: {user: @user, owner: current_user}))%>');
<% when 'note' %>
	$('#pageslide').html('<%= escape_javascript(render(partial: "/diagnostics/form_for_add_note", locals: {user: @user, owner: current_user}))%>');
<% when 'vital_signs' %>
	$('#pageslide').html('<%= escape_javascript(render(partial: "/vital_signs/form", locals: {user: @user, owner: current_user}))%>');
	//$('#pageslide').toggle();
<% end %>