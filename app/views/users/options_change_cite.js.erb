<% case @type %>
	<% when  'change_cite' %> 
		$('#change<%= @cite.id %>').html('<%= escape_javascript(render(partial: "/users/dates/update_dates", locals: {cite: @cite}))%>');
	<% when  'messages' %>
	    $('#message_load').html('<%= escape_javascript(render(partial: "/users/dates/messages", locals: {cite: @cite, messages:  @messages}))%>');
	       $('#calendar_tab').removeClass('active');
	       $('#calendar_message').addClass('active');
	       $('#message_load').show();
	       $('#calendar-init').hide();
	<% when  'cancel' %>
<%end%>

