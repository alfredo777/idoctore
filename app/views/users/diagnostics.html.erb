<% if  session[:assistant] == nil %>
<% if !agent %>
<div class="menu-select">
  <p style="font-size:30px; margin-top:20px;"><%= t('diagnostics.diagnostics')%> - <%= link_to "#{@user.name}", user_path(@user.id) %>
  </p>
</div>
<%else%>
<div class="mobile_head">
  <%= t('diagnostics.diagnostics')%>
</div>
<%end%>
<%else%>
<div class="act_head_lef_assistent">
  <%= t('diagnostics.diagnostics')%> - <%= "#{@user.name}" %>
</div>
<%end%>
<center>
<table>
  <thead>
    <tr>
      <th width="300"><%= t('diagnostics.diagnostics')%></th>
      <% if current_user.role == 'doctor' %>
      <th width="200"> <%= t('diagnostics.request_deletion')%></th>
      <%end%>
    </tr>
  </thead>
  <tbody>
    <% @diagnostics.reverse.each do |dt| %>
    <%= render partial: '/diagnostics/show', locals: {dt: dt, all_options: true}%>
    <%end%>
  </tbody>
</table>
<ul class="pagination">
  <%= will_paginate @diagnostics %>
</ul>
</center>